#ALGORITHM SORTING (10)

EXERCISE 1 Sorting find the median
def findMedian(arr):
    arr.sort()
    t= len(arr)//2
    return (arr[t])
    
EXERCISE 2 Sorting running time 
def runningTime(arr):
    count=0
    a=len(arr)
    for x in range(a):
        for i in range(x,a):
            if arr[x]>arr[i]:
                count+=1
    return count

EXERCISE 3 Quicksort1
def quickSort(arr):
    l=len(arr)
    a=list()
    b=list()
    if l <=1:
        return arr
    else:
        p= arr.pop()
    for i in arr:
        if i>p:
            a.append(i)
        else:
            b.append(i)
    t= quickSort(b)+[p]+quickSort(a)
    return t

EXERCISE 4 Counting sort 1
def countingSort(arr):
    count= [0]*100
    for i in arr:
        count[i] +=1
    return count
        
EXERCISE 5 Counting sort 2
def countingSort(arr):
    count=[0] * (max(arr)+1)
    for x in arr:
        count[x] +=1
    a=list()
    for i in range (len(count)):
        while count[i] !=0:
            count[i] -=1
            a.append(i)
    return a

EXERCISE 6 Insertion sort 1
def insertionSort1(n, arr):
    a= arr[-1]
    i= n-1
    while i >0 and arr[i-1] > a:
        arr[i]=arr[i-1]
        print(*arr)
        i-=1
    arr[i]=a
    print(*arr)

EXERCISE 7 Insertion sort2
def insertionSort2(n, arr):
    for i in range(1,n):
        a= arr[i]
        t=i      
        while t>0 and arr[t-1]>a:
            arr[t]=arr[t-1]
            #print(*arr)
            t-=1
        arr[t]=a
        print(*arr)

EXERCISE 8 Big sorting

def bigSorting(unsorted):
    t=sorted(unsorted,key=lambda x:(len(x),x))
    return t

EXERCISE 9 Sorting Intro 
def introTutorial(V, arr):
    t=arr.index(V)
    return t

EXERCISE 10 Closest Number 
def closestNumbers(arr):
    arr.sort()
    a= list()
    p=float("inf")
    for i in range(len(arr)-1):
        p=min(arr[i+1] - arr[i], p)
    for i in range(len(arr)-1):
        if arr[i+1]-arr[i]==p:
            a.append(arr[i])
            a.append(arr[i+1])
    return a
