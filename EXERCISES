#BASIC DATA TYPES(5)

EXERCISE 1 List Comprehensions
x = int(input())
y = int(input())
z = int(input())
n = int(input())
s=list()
for i in range(x+1):
    for u in range(y+1):
        for t in range(z+1):
            if (i+u+t) != n:
                s.append([i,u,t])
print(s)

EXERCISE 2 Basic data types
N = int(input())
r= list()
for x in range(N):
    l= list(input().split())
    for x in range(1,len(l)):
        l[x]=int(l[x])
    if l[0]=="append":
        r.append(l[1]) 
    if l[0]=="insert":
        r.insert(l[1],l[2])
    if l[0]=="print":
        print(r)
    if l[0]=="remove":
        r.remove(l[1])
    if l[0]=="sort":
        r.sort()
    if l[0]=="pop":
        r.pop()
    if l[0]=="reverse":
        r.reverse()

