num_list= list(range(1,100+1))
if n in num_list: 
    if (n % 2)== 0: 
        if (6<=n<=20):
            print("Weird")
        elif (2<=n<=5) | (n>20): 
            print("Not Weird")
    else:
        print("Weird")        
