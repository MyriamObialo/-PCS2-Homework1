#EXERCISE 4 Find the Runner-Up Score!
n=int(input())
arr=list(map(int,input().split()))

if int(n)==len(arr):
    if 2<=n<=10 and -100<=len(arr)<=100:
        the_max= max(arr)
        arr2= list(dict.fromkeys(arr))
        for x in arr2:
            if x== the_max:
                arr2.remove(x)
print(max(arr2))

EXERCISE 5 Finding the percentage
n = int(input())
student_marks= {}
for _ in range(n):
    name, *line = input().split()
    scores = list(map(float, line))
    student_marks[name] = scores
query_name = input()
if query_name in student_marks:
    b= list(student_marks[query_name])
    print(b)
    t=float(sum(b))
    print(t)
    print(format(sum(b)/3,".2f"))

